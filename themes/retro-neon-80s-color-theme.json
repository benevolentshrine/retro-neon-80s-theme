    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#9D4EDD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
                "constant.language.boolean",
                "constant.language.null"
            ],
            "settings": {
                "foreground": "#FF9E00"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#FF006E",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator",
            "scope": [
                "keyword.operator",
                "punctuation.separator",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "name": "Function",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#00F5FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Class",
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#B026FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#e0e0ff"
            }
        },
        {
            "name": "Parameter",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#FFB84D"
            }
        },
        {
            "name": "Property",
            "scope": [
                "variable.other.property",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#FF006E"
            }
        },
        {
            "name": "Attribute",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#B026FF"
            }
        },
        {
            "name": "Import/Export",
            "scope": [
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from"
            ],
            "settings": {
                "foreground": "#FF006E",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant.language",
                "constant.character",
                "variable.language.this"
            ],
            "settings": {
                "foreground": "#FF9E00",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition.string",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "name": "Markdown Heading",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#FF006E",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#00F5FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#B026FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Link",
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#00F5FF",
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#39FF14"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "name": "CSS Class",
            "scope": [
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "foreground": "#B026FF"
            }
        },
        {
            "name": "CSS ID",
            "scope": [
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#FF006E"
            }
        },
        {
            "name": "CSS Property",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#00F5FF"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF006E",
                "fontStyle": "bold underline"
            }
        }
    ]
}